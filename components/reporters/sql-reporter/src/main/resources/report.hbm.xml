<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.jhuapl.dorset.reporting">
	<class name="Report" table="report">
		<id name="timestamp" column="timestamp" ></id>
		<property name="routeTime" column="routeTime" />
		<property name="agentTime" column="agentTime" />
		<property name="requestText" column="requestText" /> 
<!-- 	<property name="agentNames" column="agentNames" /> -->
 		<property name="selectedAgentName" column="selectedAgentName" />
		<property name="responseText" column="responseText" />
	</class>
</hibernate-mapping>